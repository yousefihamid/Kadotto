
@{
    ViewBag.Title = "Partition";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@section scripts
{
    <script src="~/Scripts/Kadotto/Partition/PartitionDetails.js"></script>
}
    <form id="info-form" class="form-horizontal" data-toggle="validator" role="form">
        <div class="form-group">
            <label class="control-label col-md-2" for="Title">Title</label>
            <div class="col-md-10">
                <input class="form-control text-box single-line" id="Title" type="text" required="required">
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-2" for="PartitionSize">PartitionSize</label>
            <div class="col-md-10">
                <input class="form-control text-box single-line" id="PartitionSize" type="text" required="required">
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <button type="submit" value="Create" class="btn btn-default" >Save</button>
            </div>
        </div>
    </form>
<div>
    @Html.ActionLink("Back to List", "Index")
</div>